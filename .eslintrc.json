{
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 8
    }, 
    "rules": {
        "linebreak-style": 0,
        "global-require": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        // "indent": [2, "tab"],
        "space-before-function-paren": [2, "never"],
        "func-names": 0,
        "max-len": [1, {"code": 160}],
        "no-restricted-syntax": 0,
        "no-tabs": 0
    }
}